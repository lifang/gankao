<script type="text/javascript" src="/javascripts/plugin_assets/My97DatePicker/WdatePicker.js" ></script>
<div class="main_box_back">
  <div class="in">
    <% if @old_lists.blank? %>
      <h1>您目前没有参加任何考试</h1>
    <% else %>
      <div class="main">
        <div class="main_con">
          <div class="zhenti_box">
            <ul>
              <% @old_lists.each do |examination| %>
                <% unless @hash["#{examination.id}"].nil?%>
                  <% if @hash["#{examination.id}"].is_submited.nil?||@hash["#{examination.id}"].is_submited==0%>
                    <li><a href="/user/examinations/<%= examination.id %>/do_exam" target="_blank"><span class="paper_time">
                          <%= examination.title %></span><span class="paper_over"></span></a></li>
                        <% else %>
                    <li><a href="/user/exam_users/<%= examination.id %>?user_id=<%= cookies[:user_id] %>" target="_blank"><span class="paper_time">
                          <%= examination.title %></span><span class="paper_over"><img alt="真题" src="/images/paper/paper_over.png" /></span></a></li>
                        <% end %>
                      <% else %>
                  <li><a href="/user/examinations/<%= examination.id %>/do_exam" target="_blank"><span class="paper_time">
                        <%= examination.title %></span><span class="paper_over"></span></a></li>
                      <% end %>
                    <% end %>
            </ul>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>

